{
  "name": "digital_guidelines",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "config": {
    "output": "docs/storybook",
    "input": "dg"
  },
  "scripts": {
    "dg-dev": "start-storybook -s $npm_package_config_input -c storybook_config -p 58044",
    "dg-build": "build-storybook -s $npm_package_config_input -c storybook_config -o $npm_package_config_output",
    "start": "npm run dg-dev",
    "build": "npm run dg-build && open $npm_package_config_output/index.html",
    "visual-diff": "percy-storybook  --build_dir $npm_package_config_output --widths=320,400,600,800,1100,1300,1600 --enable_javascript"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-viewport": "^5.0.10",
    "@storybook/html": "^5.0.10",
    "babel-loader": "^8.0.5",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "@axa-ch/materials": "^1.0.0-alpha.3",
    "@axa-ch/patterns-library": "2.0.1-beta.264",
    "@percy/storybook": "^3.0.2",
