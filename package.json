{
	"version": "0.1.2",
	"description": "",
	"main": "index.js",
	"config": {
		"output": "./docs",
		"input": "./dg",
		"story_config": "./storybook",
		"dist": "./dist"
	},
	"scripts": {
		"dg-dev": "start-storybook -s $npm_package_config_input -p 58044",
		"dg-build": "build-storybook -s $npm_package_config_input -o $npm_package_config_output --quiet",
		"start": "npm run dg-dev",
		"build": "npm run dg-build && open $npm_package_config_output/index.html",
		"lib-build": "npx webpack --config webpack-build-config.js",
		"lib-serve": "npx http-server $npm_package_config_dist & open http://localhost:8080/demo.html",
		"test": "npm run visual-diff",
		"visual-diff": "percy-storybook  --build_dir $npm_package_config_output --widths=320,400,600,800,1100,1300,1600 --enable_javascript",
		"tool-update": "npx npm-check --save-exact --update --ignore @babel/*,*-loader,node-sass,webpack --specials=babel,webpack"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.6.0",
		"@babel/core": "^7.6.2",
		"@babel/plugin-transform-arrow-functions": "^7.2.0",
		"@babel/preset-env": "^7.6.2",
		"@babel/preset-react": "^7.0.0",
		"@percy-io/in-percy": "^0.1.11",
		"@percy/storybook": "^3.2.0",
		"@storybook/addon-notes": "^5.1.11",
		"@storybook/addon-viewport": "^5.1.11",
		"@storybook/html": "^5.1.11",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"faker": "^4.1.0",
		"file-loader": "^4.2.0",
		"html-webpack-plugin": "^3.2.0",
		"http-server": "^0.11.1",
		"mini-css-extract-plugin": "^0.8.0",
		"mockdate": "^2.0.5",
		"node-sass": "^4.11.0",
		"npm-check": "^5.9.0",
		"prismjs": "^1.17.1",
		"raw-loader": "^3.1.0",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"resolve-url-loader": "^3.1.0",
		"sass-loader": "^7.3.1",
		"style-loader": "^1.0.0",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.8",
		"webpack-fix-style-only-entries": "^0.3.1"
	},
	"dependencies": {
		"@webcomponents/custom-elements": "^1.2.4",
		"@webcomponents/webcomponentsjs": "^2.3.0",
		"awesomplete": "^1.1.5",
		"eligrey-classlist-js-polyfill": "^1.2.20180112"
	}
}
