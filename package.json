    "@storybook/addon-storysource": "^5.0.10",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fs": "0.0.1-security",
    "glob": "^7.1.3",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uppercamelcase": "^3.0.0",
    "webpack": "^4.30.0"
  }
